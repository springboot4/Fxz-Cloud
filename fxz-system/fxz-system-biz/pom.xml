<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <artifactId>fxz-system</artifactId>
        <groupId>com.fxz</groupId>
        <version>0.0.1</version>
    </parent>

    <modelVersion>4.0.0</modelVersion>
    <artifactId>fxz-system-biz</artifactId>

    <dependencies>

        <dependency>
            <groupId>com.fxz</groupId>
            <artifactId>mall-member-api</artifactId>
            <version>0.0.1</version>
        </dependency>

        <dependency>
            <groupId>p6spy</groupId>
            <artifactId>p6spy</artifactId>
        </dependency>

        <!--nacos配置中心-->
        <dependency>
            <groupId>com.alibaba.cloud</groupId>
            <artifactId>spring-cloud-starter-alibaba-nacos-config</artifactId>
        </dependency>

        <!--nacos服务发现-->
        <dependency>
            <groupId>com.alibaba.cloud</groupId>
            <artifactId>spring-cloud-starter-alibaba-nacos-discovery</artifactId>
        </dependency>

        <!--mysql链接-->
        <dependency>
            <groupId>mysql</groupId>
            <artifactId>mysql-connector-java</artifactId>
            <scope>runtime</scope>
        </dependency>

        <!-- LB 扩展 -->
        <dependency>
            <groupId>org.springframework.cloud</groupId>
            <artifactId>spring-cloud-starter-loadbalancer</artifactId>
        </dependency>

        <!--接口api-->
        <dependency>
            <groupId>com.fxz</groupId>
            <artifactId>fxz-system-api</artifactId>
        </dependency>

        <!--测试:es模块-->
        <dependency>
            <groupId>com.fxz</groupId>
            <artifactId>fxz-common-es</artifactId>
        </dependency>

        <!--测试:发号器模块-->
        <dependency>
            <groupId>com.fxz</groupId>
            <artifactId>fxz-common-sequence</artifactId>
        </dependency>

        <!--测试:websocket-->
        <dependency>
            <groupId>com.fxz</groupId>
            <artifactId>fxz-common-websocket</artifactId>
        </dependency>

        <!--测试:rabbitmq-->
        <dependency>
            <groupId>com.fxz</groupId>
            <artifactId>fxz-common-mq-rabbit</artifactId>
        </dependency>

        <!--阿里云短信服务-->
        <dependency>
            <groupId>com.fxz</groupId>
            <artifactId>fxz-common-sms</artifactId>
        </dependency>

        <!--jackson序列化-->
        <dependency>
            <groupId>com.fxz</groupId>
            <artifactId>fxz-common-jackson</artifactId>
        </dependency>

        <!--数据权限-->
        <dependency>
            <groupId>com.fxz</groupId>
            <artifactId>fxz-common-data-permission</artifactId>
        </dependency>

        <!--id幂等性-->
        <dependency>
            <groupId>com.fxz</groupId>
            <artifactId>fxz-common-idempotent</artifactId>
        </dependency>

        <!--日志-->
        <dependency>
            <groupId>com.fxz</groupId>
            <artifactId>fxz-common-log</artifactId>
        </dependency>

        <!--文件存储-->
        <dependency>
            <groupId>com.fxz</groupId>
            <artifactId>fxz-common-file</artifactId>
        </dependency>

        <!--多级缓存、分布式锁-->
        <dependency>
            <groupId>com.fxz</groupId>
            <artifactId>fxz-common-redis</artifactId>
        </dependency>

        <!--安全模块-->
        <dependency>
            <groupId>com.fxz</groupId>
            <artifactId>fxz-common-security</artifactId>
        </dependency>

    </dependencies>

    <build>
        <finalName>fxz-server-system</finalName>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
            </plugin>
        </plugins>
    </build>

</project>